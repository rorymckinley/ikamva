<%= 
FasterCSV.generate(:headers => ["First Name", "Surname", "Grade", "Attendance Record", "Percentage Attendance"], :write_headers => true) do |csv|
  @report_result[:report].each do |record|
    csv << [record[:first_name], record[:surname], record[:grade], record[:attendance_record], record[:percentage_attendance]]
  end
end
%>
